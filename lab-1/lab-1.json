[[{"key":"Downloads","value":"1"},{"key":"Downloads","value":"4496832"}],[{"value":"2"},{"value":"4676943"}],[{"value":"3"},{"value":"4385410"}],[{"value":"4"},{"value":"4104789"}],[{"value":"5"},{"value":"3860386"}],[{"value":"6"},{"value":"3184789"}],[{"value":"7"},{"value":"3110815"}]],"columnNames":[{"name":"Day","index":0,"aggr":"sum"},{"name":"Downloads","index":1,"aggr":"sum"}],"rows":[["1","4496832"],["2","4676943"],["3","4385410"],["4","4104789"],["5","3860386"],["6","3184789"],["7","3110815"]]},"focus":true},{"title":"Segunda Parte: Funciones","text":"/** Carga un archivo contiene el mapeo prefix -> publisher.\n*  \n*  @param \tfile\t\t\t\t\tPath al archivo.\n*  @return\tRDD[(String,String)] \t\n*/\ndef loadPrefixMapping(file : String): RDD[(String, String)] = {\n    val reader = CSVReader.open(new File(file))\n    \n    val result = sc.parallelize(reader.all().tail.map(x => (x(2), x(1))))\n    \n    reader.close()\n    \n    result\n}\n\n/** Case class para manejar los metadatos\n */\ncase class Metadata(doi: String = \"\", title: String = \"\", year: Int = -1, type_ : String = \"\",\n    publisher: String = \"\", book: String = \"\", subject: List[String] = Nil) {\n    def toTabString: String = List(doi, title, year, type_, publisher, book, subject.mkString(\", \")).mkString(\"\\t\")\n}\n\n/** Retorna la metadata asociada a un DOI\n*  \n*  @param \tdoi\t\t\t\t\tDOI\n*  @return\tMetadata \t\n*/\ndef getMetadata(doi:String) = {\n    val response = Http(s\"http://dx.doi.org/$doi\").header(\"Accept\", \"application/citeproc+json\").option(HttpOptions.followRedirects(true)).asString\n    \n    if (response.code == 200) {\n        val metadata = parse(response.body)\n    \n        Metadata(\n            doi,\n            (metadata \\ \"title\").extract[String],\n            DateTime.parse((metadata \\ \"created\" \\ \"date-time\").extract[String]).getYear,\n            (metadata \\ \"type\").extract[String],\n            (metadata \\ \"publisher\").extract[String],\n            (metadata \\ \"container-title\").extract[String],\n            (metadata \\ \"subject\").extract[List[String]]\n        )\n    } else {  // Something went wrong (most likely the doi doesn't exists)\n        Metadata() // metadata with default values\n    }\n}\n\ndef aggregateByDoi(raw : RDD[Array[String]]): RDD[(String, Int)] = raw.map(x => (x(1), 1)).reduceByKey(_ + _).sortBy(p => (-p._2, p._1))\n\ndef aggregateByPrefix(raw : RDD[Array[String]]): RDD[(String, Int)] = raw.map(d => (d(1).split(\"/\")(0), 1)).reduceByKey(_ + _).sortBy(p => (-p._2, p._1))\n\ndef aggregateByCountry(raw : RDD[Array[String]]) : RDD[(String, Int)] = raw.map(x => (x(3), 1)).reduceByKey(_ + _).sortBy(p => (-p._2, p._1))","dateUpdated":"2016-10-13T18:29:49-0300","config":{"tableHide":false,"colWidth":12,"editorMode":"ace/mode/scala","editorHide":false,"title":true,"graph":{"mode":"table","height":300,"optionOpen":false,"keys":[],"values":[],"groups":[],"scatter":{}},"enabled":true},"settings":{"params":{},"forms":{}},"jobName":"paragraph_1475135542727_-515303708","id":"20160929-095222_582722188","dateCreated":"2016-09-29T09:52:22-0300","dateStarted":"2016-10-13T18:29:49-0300","dateFinished":"2016-10-13T18:29:53-0300","status":"FINISHED","progressUpdateIntervalMs":500,"$$hashKey":"object:2611","result":{"code":"SUCCESS","type":"TEXT","msg":"\nloadPrefixMapping: (file: String)org.apache.spark.rdd.RDD[(String, String)]\n\ndefined class Metadata\n\ngetMetadata: (doi: String)Metadata\n\naggregateByDoi: (raw: org.apache.spark.rdd.RDD[Array[String]])org.apache.spark.rdd.RDD[(String, Int)]\n\naggregateByPrefix: (raw: org.apache.spark.rdd.RDD[Array[String]])org.apache.spark.rdd.RDD[(String, Int)]\n\naggregateByCountry: (raw: org.apache.spark.rdd.RDD[Array[String]])org.apache.spark.rdd.RDD[(String, Int)]\n"},"focus":true},{"title":"Segunda Parte: Ejecucion","text":"val prefixes: scala.collection.Map[String, String] = loadPrefixMapping(s\"${base_dir}/publisher_DOI_prefixes.csv\").col